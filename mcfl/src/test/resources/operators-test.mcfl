$let x = 10
$let y = 20

$debugger "-x    = ${-x}"
$debugger "+x    = ${+x}"
$debugger "++x   = ${++x}"
$debugger "--x   = ${--x}"
$debugger "x--   = ${x--}"
$debugger "x++   = ${x++}"
$debugger "x+y   = ${x+y}"
$debugger "x-y   = ${x-y}"
$debugger "x*y   = ${x*y}"
$debugger "x/y   = ${x/y}"
$debugger "x|y   = ${x|y}"
$debugger "x%y   = ${x%y}"
$debugger "x&y   = ${x&y}"
$debugger "x<<y  = ${x<<y}"
$debugger "x<y   = ${x<y}"
$debugger "x>y   = ${x>y}"
$debugger "x>=y  = ${x>=y}"
$debugger "x<=y  = ${x<=y}"
$debugger "x<<<y = ${x<<<y}"
$debugger "x>>y  = ${x>>y}"
$debugger "x>>>y = ${x>>>y}"